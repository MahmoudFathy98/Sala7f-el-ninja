class room
{
   int rnum;
   int bednum;
   String rclass;
   String rview;
   int rprice;
}


class hotel
{
   String hname;
   int hrate;
   String hdestination;
   String hwebsite;
   int hroomsnumber;
   int htelephone;
}


class guestinformation 
{
    String firstname;
    String lastname;
    int id;
    int age;
    String adress;
    long phonenumber;
    String email;
    double deposit;
}    


class reservation 
{
    int reservationnumber;
    int roomnumber;
    int agentname;
    int checkindate;
    int checkoutdate;
    double roomprice;
    String spa;
    String trasportaion;
    String gym;
    String safari;   
}


class Employee
{
    int ID;
    String FirstName;
    String LastName;
    String Address;        
    String PhoneNumber;
    String Email;
    int Salary;
}


class Payment
{
    enum Payment { Cash, CreditCard, BankTransfer };
    int TotalCost;
}
